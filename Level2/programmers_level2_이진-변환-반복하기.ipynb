   "outputs": [],
   "source": [
    "s = '110010101001'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(s):\n",
    "    result = [0] * 2\n",
    "    while s != '1':\n",
    "        result[0] += 1\n",
    "        result[1] += s.count('0')\n",
    "        s = bin(s.count('1'))[2:]\n",
    "    \n",
    "    return result"
   ]
  },
