   "source": [
    "# 프로그래머스 - 숫자의 표현"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 15"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    \n",
    "    cnt = 0\n",
    "    for i in range(1, int(n // 2) + 1):\n",
    "        x = i\n",
    "        sum = 0\n",
    "\n",
    "        while sum < n :\n",
    "            sum += x\n",
    "            x += 1\n",
    "\n",
    "        if sum == n:\n",
    "            cnt += 1\n",
    "\n",
    "    return cnt + 1"
   ]
  }
 ],
