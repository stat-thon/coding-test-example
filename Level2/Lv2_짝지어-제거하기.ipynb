   "source": [
    "s = 'baabaa'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 106,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(s):\n",
    "\n",
    "    if len(s) % 2 != 0: return 0\n",
    "\n",
    "    renew = ['']\n",
    "    for i in s:\n",
    "        if i != renew[-1]:\n",
    "            renew.append(i)\n",
    "        else:\n",
    "            renew.pop()\n",
    "    \n",
    "    return 1 if renew == [''] else 0"
   ]
  }
