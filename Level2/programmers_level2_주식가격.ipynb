   "outputs": [],
   "source": [
    "prices = [2, 3, 5, 3, 1, 2, 7, 8, 7]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(prices):\n",
    "    a = []\n",
    "    for i in range(len(prices)):\n",
    "        for j in range(i + 1, len(prices)):\n",
    "            if prices[i] > prices[j]:\n",
    "                break\n",
    "        a.append(j - i)\n",
    "    return a"
   ]
  }
