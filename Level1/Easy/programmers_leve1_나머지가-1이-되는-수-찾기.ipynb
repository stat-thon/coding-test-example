   "source": [
    "# 프로그래머스 - 나머지가 1이 되는 수 찾기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 10"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "\n",
    "    x = 0\n",
    "    while True:\n",
    "        x += 1\n",
    "        if n % x == 1:\n",
    "            result = x\n",
    "            break\n",
    "    return result"
   ]
  }
